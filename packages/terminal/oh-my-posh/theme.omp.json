{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "$PROMPT$",
          "style": "plain",
          "template": "➜",
          "type": "text"
        },
        {
          "foreground": "$PATH$",
          "properties": {
            "style": "agnoster"
          },
          "style": "plain",
          "template": " {{ path .Path .Location }} ",
          "type": "path"
        },
        {
          "foreground": "$GIT$",
          "properties": {
            "cherry_pick_icon": "\u2713 ",
            "commit_icon": "\u25b7 ",
            "fetch_status": true,
            "merge_icon": "\u25f4 ",
            "no_commits_icon": "[no commits]",
            "rebase_icon": "\u2c62 ",
            "tag_icon": "\u25b6 "
          },
          "style": "plain",
          "template": "| {{ .HEAD }}@{{ url .RepoName .UpstreamURL }} <$PROMPT$>{{ if and (eq .Ahead 0) (eq .Behind 0) }}<b>\u2261</b>{{end}}{{ if gt .Ahead 0 }}<b>\u2191</b>{{.Ahead}}{{end}}{{ if gt .Behind 0 }}<b>\u2193</b>{{.Behind}}{{end}}</> <$ERROR$>{{ if .Working.Changed }}+{{ .Working.Added }} ~{{ .Working.Modified }} -{{ .Working.Deleted }} {{ end }}</><$SUCCESS$>{{ if .Staging.Changed }}+{{ .Staging.Added }} ~{{ .Staging.Modified }} -{{ .Staging.Deleted }} {{ end }}</>| ",
          "type": "git"
        },
        {
          "foreground": "$SUCCESS$",
          "foreground_templates": ["{{ if gt .Code 0 }}$ERROR${{ end }}"],
          "properties": {
            "always_enabled": true,
            "display_exit_code": true
          },
          "style": "plain",
          "template": "{{ if gt .Code 0 }}\uf00d {{ .Meaning }}{{ else }}{{ end }}",
          "type": "exit"
        },
        {
          "foreground": "$PROMPT$",
          "style": "plain",
          "template": " <b>$</b>",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
