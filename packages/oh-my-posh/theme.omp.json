{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#ffcb6b",
          "style": "plain",
          "template": "➜",
          "type": "text"
        },
        {
          "foreground": "#2266aa",
          "properties": {
            "style": "agnoster"
          },
          "style": "plain",
          "template": " {{ path .Path .Location }} ",
          "type": "path"
        },
        {
          "foreground": "#4288ee",
          "properties": {
            "cherry_pick_icon": "\u2713 ",
            "commit_icon": "\u25b7 ",
            "fetch_status": true,
            "merge_icon": "\u25f4 ",
            "no_commits_icon": "[no commits]",
            "rebase_icon": "\u2c62 ",
            "tag_icon": "\u25b6 "
          },
          "style": "plain",
          "template": "| {{ .HEAD }}@{{ url .RepoName .UpstreamURL }} {{ if and (eq .Ahead 0) (eq .Behind 0) }}\u2261{{end}}{{ if gt .Ahead 0 }}\u2191{{.Ahead}}{{end}}{{ if gt .Behind 0 }}\u2193{{.Behind}}{{end}} {{ if .Working.Changed }}+{{ .Working.Added }} ~{{ .Working.Modified }} -{{ .Working.Deleted }} {{ end }}| ",
          "type": "git"
        },
        {
          "foreground": "#83c85d",
          "foreground_templates": ["{{ if gt .Code 0 }}#ee5168{{ end }}"],
          "properties": {
            "always_enabled": true,
            "display_exit_code": true
          },
          "style": "plain",
          "template": "{{ if gt .Code 0 }}\uf00d {{ .Meaning }}{{ else }}{{ end }}",
          "type": "exit"
        },
        {
          "foreground": "#ffcb63",
          "style": "plain",
          "template": " <b>$</b>",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
